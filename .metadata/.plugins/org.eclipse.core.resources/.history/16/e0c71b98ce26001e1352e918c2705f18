package edu.kh.array.practice;

import java.util.Scanner;

public class PracticeService {
	public void practice1() {
		int[] arr = new int[9];
		int sum = 0;
		
		for(int i = 0; i < arr.length; i++) {
			System.out.print((i + 1) + " ");
			
			if(arr.length % 2 == 0) {
				sum += arr[i];
		}
			
		}
		System.out.println(sum);
	}
	
}
