package edu.kh.array.practice;

import java.util.Scanner;

public class PracticeService {
	public void practice1() {
		int[] arr = {1, 2, 3, 4, 5, 6, 7, 8, 9};
		int sum = 0;
		
		for(int i = 0; i < arr.length; i++) {
			System.out.print((i + 1) + " ");
			
			if((i + 1)% 2 == 0) {
				sum += i;
		}
			
		}
		System.out.println(sum);
	}
	
}
